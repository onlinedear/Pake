{
  "package": {
    "productName": "Ai-Art",
    "version": "1.0.0"
  },
  "tauri": {
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "https://www.ai-art.com.cn/?MidjourneyCN/",
          "windows": [
            "Ai-Art"
          ],
          "enableTauriAPI": true
        }
      ]
    },
    "updater": {
      "active": false
    },
    "systemTray": {
      "iconPath": "png/ai-art_512.png",
      "iconAsTemplate": true
    },
    "allowlist": {
      "all": true,
      "fs": {
        "all": true,
        "scope": [
          "$DOWNLOAD/*"
        ]
      }
    }
  },
  "build": {
    "withGlobalTauri": true,
    "devPath": "../dist",
    "distDir": "../dist",
    "beforeBuildCommand": "",
    "beforeDevCommand": ""
  }
}
